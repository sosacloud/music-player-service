{
  "name": "music-player-service",
  "version": "1.0.0",
  "description": "music player service for SoundCloud song page",
  "main": "./client/index.jsx",
  "engines": {
    "node": ">= 12.6.0",
    "npm": ">= 6.10.2"
  },
  "scripts": {
    "db:dev": "mysql -u root < schema.sql",
    "seed:dev": "node database/seed.js",
    "start:dev": "nodemon server/index.js",
    "build:dev": "webpack -d --watch",
    "dev": "concurrently \"npm run db:dev\" \"npm run seed:dev\" \"npm run start:dev\" \"npm run build:dev\"",
    "start": "node server/index.js",
    "heroku-postbuild": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sosacloud/music-player-service.git"
  },
  "author": "underthecode",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sosacloud/music-player-service/issues"
  },
  "homepage": "https://github.com/sosacloud/music-player-service#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-timeago": "^4.4.0",
    "sequelize": "^5.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  }
}
