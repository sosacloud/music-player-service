{
  "name": "music-player-service",
  "version": "1.0.0",
  "description": "music player service for SoundCloud song page",
  "main": "./client/index.jsx",
  "engines": {
    "node": ">= 12.4.0",
    "npm": ">= 6.9.0"
  },
  "scripts": {
    "start:dev": "nodemon server/index.js",
    "db:dev": "mysql -u root < schema.sql",
    "build:dev": "webpack -d --watch",
    "seed:dev": "node database/seed.js",
    "dev": "concurrently \"npm run start:dev\" \"npm run db:dev\" \"npm run build:dev\" \"npm run seed:dev\"",
    "start": "node database/seed.js",
    "heroku-postbuild": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sosacloud/music-player-service.git"
  },
  "author": "underthecode",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sosacloud/music-player-service/issues"
  },
  "homepage": "https://github.com/sosacloud/music-player-service#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "mysql": "^2.17.1",
    "nodemon": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sequelize": "^5.8.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  }
}
